    <script>

        $(document).ready(function () {
            SetCrousel();
            $(".search-input").keypress(function (event) {
                var keycode = (event.keyCode ? event.keyCode : event.which);
                if (keycode == '13') {
                    Filter();
                }
            })

            $(".search-input").keyup(function (event) {
                var value = $(this).val();
                if (value == '') {
                    Filter();
                }
            })
        })

        function SetCrousel() {
            $('.carousel-showmanymoveone').carousel({ interval: 900000 });
            $('#carouselABC').carousel({ interval: 500000 });

            $('.carousel-showmanymoveone .item').each(function () {
                var itemToClone = $(this);
                for (var i = 1; i < 4; i++) {
                    itemToClone = itemToClone.next();

                    // wrap around if at end of item collection
                    if (!itemToClone.length) {
                        itemToClone = $(this).siblings(':first');
                    }

                    // grab item, clone, add marker class, add to collection
                    itemToClone.children(':first-child').clone()
                        .addClass("cloneditem-" + (i))
                        .appendTo($(this));
                }
            });
        }

        function ShowPersonModal(sellerId, user) {
            $.ajax({
                url: "../User/GetUserDetail",
                data: { userId: sellerId },
                contentType: "json",
                success: function (data) {
                    $("#PersonModal .user-name").text(data.FullName);
                    $("#PersonModal .user-email").text(data.Email);
                    $("#PersonModal .user-pcontact").text(data.MobileNo);
                    $("#PersonModal .user-scontact").text(data.ProductSubChildGroupName);
                    $("#PersonModal .user-whatsapp").text(data.SpecificationName);
                    $("#PersonModal .user-fax").text(data.SpecificationValue);
                    $("#PersonModal .user-state").text(data.UOMName);
                    $("#PersonModal .user-city").text(data.UOMName);
                    $("#PersonModal .user-subcity").text(data.UOMName);
                    $("#PersonModal .user-landmark").text(data.Landmark);
                    $("#PersonModal .user-building").text(data.Building);
                    $("#PersonModal .user-pincode").text(data.PinCode);
                    $("#PersonModal .modal-title").text(user + " Details");
                    $("#PersonModal").modal();
                },
                error: function () {
                    ShowErrorModal("Product Specifications", "Error fetching details")
                }
            });
        }

        function ShowProductModal(ProductName, ProductCode, ParentClassName, CategoryName, SubCategoryName, BrandName, specifications) {
            $("#ProductSpecModal .product-name").text(ProductName);
            $("#ProductSpecModal .product-code").text(ProductCode);
            $("#ProductSpecModal .product-c1").text(ParentClassName);
            $("#ProductSpecModal .product-c2").text(CategoryName);
            $("#ProductSpecModal .product-c3").text(SubCategoryName);
            $("#ProductSpecModal .product-brand").text(BrandName);
            $(".spec-table tr").not(":first").html("");

            $(JSON.parse(specifications)).each(function () {
                var row = "<tr><td>" + this.SpecName + "</td><td>" + this.SpecValue + "</td><td>" + this.UomName + "</td><tr/>";
                $(".spec-table").append(row);
            });
            $("#ProductSpecModal").modal();
        }

        function ShowSellerPriceProductModal(sellerproductdetailsID, ProductName) {

            $("#hdnsellerproductdetailsID").val(sellerproductdetailsID);
            $("#lblpriceProductName").text(ProductName);
            getselllerproductpricedetails(sellerproductdetailsID);

            $("#ProductquatationModal").modal();
        }

        function ShowBuyerOrderProductModal(buyerproductdetailsID, ProductName) {
           
            $("#hdnbuyerproductdetailsID").val(buyerproductdetailsID);
            $("#lbldemandProductName").text(ProductName);
            getbuyerproductqtydetails(buyerproductdetailsID);

