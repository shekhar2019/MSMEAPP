        function savesellerproductprice() {

            var sellerproductdetailsID = $("#hdnsellerproductdetailsID").val();
            var sellerOrderprice = $("#txtsellerOrderprice").val();
            var MinimumOrderQuantity = $("#txtMinimumOrderQuantity").val();

            if (sellerOrderprice == "" || sellerOrderprice == "0") {
                alert("Please Enter Price");

                return false;
            }
            if (MinimumOrderQuantity == "" || MinimumOrderQuantity == "0") {
                alert("Please Enter Minimum Order Quantity");

                return false;
            }

            var requestData = {

                sellerproductdetailsID: sellerproductdetailsID,
                price: sellerOrderprice,
                MinimumOrderQuantity: MinimumOrderQuantity
            };

            $.ajax({
                url: "../SellerProduct/UpdateSellerProductPrice",
                cache: false,
                type: "POST",
                dataType: "json",
                data: JSON.stringify(requestData),
                contentType: 'application/json',
                success: function (data) {

                    alert("Price Sucessfully updated");
                    $("#txtsellerOrderprice").val("");
                    $("#txtMinimumOrderQuantity").val("");
                    getselllerproductpricedetails(sellerproductdetailsID);
                },
                error: function (err) {

                }
            });
        }

        function savebuyerproductqty() {
           
