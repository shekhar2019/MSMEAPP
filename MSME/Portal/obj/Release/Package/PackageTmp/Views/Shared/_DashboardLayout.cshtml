            var category2 = $("#btnFilterCategory").closest("div").find("input").val();
            var category3 = $("#btnFilterCategory").closest("div").find("input").val();
            var city = $("#btnFilterCity").closest("div").find("input").val();
            var sortBy = $("#ddlSortBy").val();
            var url = $("#UpdatedDataUrl").val();

            $.ajax(
                {
                    url: url + "?category=" + category1 + "&city=" + city + "&sortBy=" + sortBy,
