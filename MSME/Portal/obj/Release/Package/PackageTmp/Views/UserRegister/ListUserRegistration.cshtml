@{
    ViewBag.Title = "List Customer";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/AccountDashboard">&nbsp; </a> User Registration </b></p></div>
</div>
<section>
    <form action="#" id="frmUserRegistration" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group  col-md-6">
                    <label for="txtFirstNameName" class="col-sm-4 control-label">Name</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtFirstNameName" name="txtFirstNameName" tabindex="1" placeholder="Search with Party Name" type="text" maxlength="100" />
                    </div>
                </div>
                <div class="form-group col-md-6">
                    <label for="ddlKYCCode" class="col-sm-4 control-label">KYC Code</label>
                    <div class="col-sm-8 ">
                        <select id="ddlKYCCode" name="ddlKYCCode" tabindex="4" class="form-control">
                            <option value="">Select KYC Code</option>
                            <option value="B">Buyer</option>
                            <option value="S">Seller</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group col-md-6">
                    <label for="txtMobileNo" class="col-sm-4 control-label">Mobile No.</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtMobileNo" name="txtMobileNo" tabindex="3" placeholder="Search with Mobile No." type="text" maxlength="20" />
                    </div>
                </div>
                <div class="form-group col-md-6">
                    <label for="txtEmail" class="col-sm-4 control-label">Email</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtEmail" name="txtEmail" tabindex="3" placeholder="Search with Email" type="text" maxlength="50" />
                    </div>
                </div>
            </div>
            

            <div class="row col-sm-12 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" onclick="SearchUserRegistration()" type="button" border="0">&nbsp;&nbsp;
                        <input class="btn btn-success" type="button" id="btnReset" onclick="ClearFields()" value="Reset" /> &nbsp;&nbsp;
                        
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/UserRegistration/ListUserRegistration.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>