
@{
    ViewBag.Title = "GasketedPHE";
    Layout = "~/Views/Shared/_LayoutWebSite.cshtml";
}

<!-- Inner Page Banner Area Start Here -->
<div class="inner-page-banner-area" style="background-image: url('img/banner/5.html');">
    <div class="container">
        <div class="pagination-area">
            @foreach (var productImages in ViewBag.productImages)
            {
                <center><h2>@productImages.ProductName</h2></center>
               
            }
            
            <ul>
                <li><a href="../Home">Home</a> -</li>
                @foreach (var productImages in ViewBag.productImages)
                {
                    <li>@productImages.ProductName</li>
                    

                }
              
            </ul>
        </div>
    </div>
</div>
<!-- Inner Page Banner Area End Here -->
<!-- Courses Page 3 Area Start Here -->
<div class="courses-page-area3">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-8 col-xs-12">
                <div class="row">
                    <div class="product-list clearfix">
                        <div class="product clearfix" id="product8">
                            <div class="col-md-12 col-xs-12">
                                @foreach (var productImages in ViewBag.productImages)
                                    {
                                        <h4 class="h4-heading">@productImages.ProductName</h4>
                                    }
                            </div>
                            <div class="col-md-3 col-xs-12">
                                <figure class="figure-class">
                                    @foreach (var productImages in ViewBag.productImagesList)
                                    {
                                        <a href="#"><img alt="Plate Heat Exchanger" src="~/Images/ProductImage/@productImages.ImageUrl"></a>

                                        <div class="overlay">
                                            <a class="zoom" href="~/Images/ProductImage/@productImages.ImageUrl"></a>
                                            <a class="link" href="#"></a>
                                        </div>

                                    }

                                </figure>
                                <a id="popup" onclick="yes_show('Please send me the latest price and details of Gasketed PHE.',9);"><input type="submit" value="Enquiry" class="btn btn-danger sub-cat-btn" style="margin-top:2px;"></a><a href="#lightbox" data-toggle="modal"><input type="submit" value="Applications" class="btn btn-danger sub-cat-btn" style="margin-top: 5px; margin-left:2px;"></a>
                                <a href="~/Media/images/products/fileUpload/Tranter%20catalogue%20-%20Gaskted%20product%20line%20detailed_eP4SX.pdf" target="_blank">
                                    <img src="~/Images/pdf/pdf.jpg" width="50" />
                                </a>
                            </div>
                            <div class="detail col-md-9">
                                @foreach (var productImages in ViewBag.productImages)
                                {
                                    <p>
                                        @Html.Raw(productImages.ProductFullDesc)
                                    </p>
                                }
                                @*<p><p>
                                        Gasketed plate heat exchanger (PHE) for quick and high efficiency heat transfer with flexibility in compact size. Can be manufactured from various plate and gasket materials suitable for industrial and corrosive applications.
                                    </p>
                                    <p>
                                        Flow ranges: 10 &ndash; 4600m3/h
                                    </p>
                                    <p>
                                        Working Pressure: Vaccum&ndash; 25 bar
                                    </p>
                                    <p>
                                        Temperature range: -20&deg;C &ndash; +180&deg;C
                                    </p>
                                    <p>
                                        Connections: DN25 &ndash; DN500 (Weld neck, flange, or threaded)
                                    </p>
                                    <p>
                                        Applications: Gasketed PHEs provides excellent heat transfer in compact area. It can be used for a wide spectrum of applications inIndustries like Oil and Gas, Petrochemical Refineries HVAC, Chemical, Food and Beverages, Thermal Power, Hydro Power, Solar Power, Plastic, Machine Tools &amp; Mechanical, Vegetable Oil, Pharmaceutical, Pulp and Paper, Sugar and Distilleries, Breweries, Hotel and many others.
                                    </p>
                                    <p>
                                        Gasketed heat exchangers can be used almost for all heat transfer applications where gaskets are suitable with fluid and pressures.
                                    </p>
                                    </p>*@

                            </div>
                        </div>



                        <div class="container">
